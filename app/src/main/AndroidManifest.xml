<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.matt.movieWatchList">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ViewControllers.Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ViewControllers.Activities.BrowseActivity">
        </activity>
        <activity
            android:name=".ViewControllers.Activities.TmdbActivity"
            android:parentActivityName=".ViewControllers.Activities.BrowseActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ViewControllers.Activities.MainActivity" />
        </activity>

    </application>

</manifest>